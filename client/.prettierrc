{
   "trailingComma": "es5",
   "semi": false,
   "useTabs":  true,
   "tabWidth": 2,
   "singleQuote": true,
   "importOrder":[
      "<THIRD_PARTY_MODULES>",
      "^@/components/(.*)$",
      "^@/screens/(.*)$",
      "^@/ui/(.*)$",
      "^@/hooks/(.*)$",
      "^@/shared/(.*)$",
      "^@/services/(.*)$",
      "^@/styles/(.*)$",
      "^@/assets/(.*)$",
      "^@/store/(.*)$",
      "^@/utils/(.*)$",
      "^@/config/(.*)$",
      "^../(.*)",
      "^./(.*)",
      "(.scss)$"
   ],
   "importOrderSeparation":true,
   "importOrderSortSpecifiers" :true

}
